   "source": [
    "#원주율은 (1/1^2) +(2/2^2) +...\n",
    "FMyPi(o,s) = return o+(s/(s^2))\n",
    "\n",
    "function FPiFor(X)\n",
    "    Y=0\n",
    "    for idx1 in 1:X\n",
    "        Y = FMyPi(Y,idx1)\n",
    "        \n",
    "    end\n",
    "    return Y\n",
    "end\n",
    "\n",
    "println(FPiFor(1000))\n"
